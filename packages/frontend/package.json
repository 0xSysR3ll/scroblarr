{
  "name": "@scroblarr/frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src && prettier --check \"src/**/*.{ts,tsx,json,css}\"",
    "lint:fix": "eslint src --fix && prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "lint:eslint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "i18n:extract": "i18next-scanner --config i18next-scanner.config.cjs"
  },
  "dependencies": {
    "@scroblarr/shared": "workspace:*",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^14.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.21.0",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/validator": "^13.11.10",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint-plugin-i18next": "^6.1.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "i18next-scanner": "^4.6.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^1.2.0"
  }
}
